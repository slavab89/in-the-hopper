  "description": "Node.js middleware for extracting data from a request",
  "main": "index.js",
  "author": {
    "name": "Slava Balabanov",
    "email": "bslava89@gmail.com"
  },
  "bugs": "https://github.com/slavab89/in-the-hopper/issues",
  "repository": {
    "type": "git",
